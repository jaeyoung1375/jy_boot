create table jy_member(
    memberId varchar2(40) primary key not null,
    memberPw varchar2(40) not null,
    memberName varchar2(21) not null,
    memberNickName varchar2(30) unique not null,
    memberEmail varchar2(50) unique not null,
    adminCk number(1) default 0,
    createDt DATE
    );

select * from jy_member order by createDt desc;

select memberId from jy_member;

insert into jy_member values('jaeyoung1375','1234','테스트1','테스트1','테스트1',0,sysdate);
commit;


-- 상품 테이블

create table product(
product_no number not null primary key,
product_name varchar2(50) not null,
product_price number not null,
product_stock number not null,
product_description varchar2(2000) not null
);

create sequence product_seq;
